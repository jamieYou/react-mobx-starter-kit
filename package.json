{
  "name": "react-mobx-starter-kit",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/jamieYou/react-mobx-starter-kit",
  "scripts": {
    "start": "npm-run-all --parallel server:dev lint:watch",
    "staging": "npm-run-all remove-dist lint create:dist:staging",
    "build": "npm-run-all remove-dist lint create:dist",
    "start:prod": "npm-run-all build server:prod",
    "server:dev": "nodemon ./script/server.js --watch script --watch config",
    "server:prod": "cross-env NODE_ENV=production node ./script/server.js",
    "create:dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js --progress",
    "create:dist:staging": "cross-env NODE_ENV=staging webpack --config ./config/webpack.prod.js --progress",
    "lint": "esw src --ext .jsx --ext .js",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rimraf ./dist"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.3",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.2",
    "core-js": "^2.5.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.4",
    "http-proxy-middleware": "^0.17.4",
    "ipv4": "^1.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.6",
    "morgan": "^1.8.1",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.0.2",
    "opn": "^5.2.0",
    "postcss-loader": "^2.1.1",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "antd-mobile": "^2.1.6",
    "core-decorators": "^0.20.0",
    "fastclick": "^1.0.6",
    "github-markdown-css": "^2.10.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.4.1",
    "mobx": "^3.6.1",
    "mobx-logger": "^0.6.0",
    "mobx-react": "^4.4.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
